{
  "name": "@warpy-auth-sdk/core",
  "version": "0.0.21",
  "description": "auth-sdk",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./typings/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./adapters/fastify": {
      "types": "./typings/adapters/fastify.d.ts",
      "import": "./dist/adapters/fastify.js",
      "default": "./dist/adapters/fastify.js"
    },
    "./adapters/express": {
      "types": "./typings/adapters/express.d.ts",
      "import": "./dist/adapters/express.js",
      "default": "./dist/adapters/express.js"
    },
    "./adapters/hono": {
      "types": "./typings/adapters/hono.d.ts",
      "import": "./dist/adapters/hono.js",
      "default": "./dist/adapters/hono.js"
    },
    "./adapters/node": {
      "types": "./typings/adapters/node.d.ts",
      "import": "./dist/adapters/node.js",
      "default": "./dist/adapters/node.js"
    },
    "./adapters/remix": {
      "types": "./typings/adapters/remix.d.ts",
      "import": "./dist/adapters/remix.js",
      "default": "./dist/adapters/remix.js"
    },
    "./hooks": {
      "types": "./typings/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "default": "./dist/hooks/index.js"
    },
    "./hooks/server": {
      "types": "./typings/hooks/server.d.ts",
      "import": "./dist/hooks/server.js",
      "default": "./dist/hooks/server.js"
    },
    "./next": {
      "types": "./typings/next/index.d.ts",
      "import": "./dist/next/index.js",
      "default": "./dist/next/index.js"
    }
  },
  "scripts": {
    "start": "node .",
    "build": "tsc && node scripts/fix-esm-imports.js",
    "dev": "ts-node-dev --respawn --no-deps --rs --cls ./src/index.ts",
    "eslint": "eslint src/**/*.ts"
  },
  "keywords": [
    "auth-sdk",
    "typescript",
    "node",
    "nodejs"
  ],
  "author": {
    "email": "jucas.oliveira@gmail.com",
    "name": "Lucas Oliveira"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.38.0",
    "eslint-plugin-prettier": "5.5.4",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "^19.2.2",
    "@warpy-auth-sdk/core": "^0.0.19",
    "ai": "^5.0.78",
    "axios": "^1.12.2",
    "cookie": "^1.0.2",
    "esbuild": "^0.25.11",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.10",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "resend": "^6.2.2",
    "vitest": "^4.0.2",
    "winston": "3.18.3",
    "zod": "^4.1.12"
  },
  "typesVersions": {
    "*": {
      "adapters/fastify": [
        "typings/adapters/fastify.d.ts"
      ],
      "adapters/express": [
        "typings/adapters/express.d.ts"
      ],
      "adapters/hono": [
        "typings/adapters/hono.d.ts"
      ],
      "adapters/node": [
        "typings/adapters/node.d.ts"
      ],
      "adapters/remix": [
        "typings/adapters/remix.d.ts"
      ],
      "hooks": [
        "typings/hooks/index.d.ts"
      ],
      "hooks/server": [
        "typings/hooks/server.d.ts"
      ],
      "next": [
        "typings/next/index.d.ts"
      ],
      "*": [
        "typings/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "typings"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typings": "./typings/index.d.ts"
}
